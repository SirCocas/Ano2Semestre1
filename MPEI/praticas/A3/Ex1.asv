function p = prob(n, targets)
    pool = randi(targets,n,1e3);
    count=0;
    counter2=0;
    for k=1:length(pool)
        u=unique(pool(:,k));
        l=length(u);
        counter2=counter2+1;
        if(l<n)
            count=count+1;
        end;
    end;
    p = count/counter2;
end